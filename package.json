{
  "name": "@blotoutio/sdk-js",
  "version": "0.5.0",
  "description": "Blotout JS SDK",
  "author": "Blotout",
  "license": "MPL-2.0",
  "homepage": "https://github.com/blotoutio/sdk-js",
  "repository": {
    "type": "git",
    "url": "git://github.com/blotoutio/sdk-js.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "webpack --config webpack.prod.js --env features=basic",
    "build:full": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "publish": "tsc --outDir dist && tsc -m es6 --outDir dist/lib-esm && webpack --config webpack.publish.js",
    "demo": "webpack --config webpack.demo.js",
    "start": "webpack serve --config webpack.dev.js",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "test": "npm-run-all test:*",
    "test:unit": "jest --runInBand",
    "test:integration": "start-server-and-test start http://localhost:9000 'cypress run'"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@blotoutio/jsencrypt-no-random-padding": "^2.3.4",
    "crypto-js": "^3.1.9-1",
    "ua-parser-js": "^0.7.23",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.3.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cypress": "^6.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "ifdef-loader": "^2.1.5",
    "jest": "^26.4.2",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.11.7",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
